    "homepage_url": "https://github.com/spijkercenter/sticky-bigquery-headers",
    "version": "1.0",
    "content_scripts": [{
        "css": ["styles.css"],
        "matches": ["https://console.cloud.google.com/bigquery*"]
    }],
    "manifest_version": 2
  }